@model SIRH.Web.Models.FuncionarioModel

@{
    ViewBag.Title = "Resultados";
    Layout = "~/Views/Shared/Home.cshtml";
}
<link rel="stylesheet" href="~/Content/sidebar.css">
@{
        if (Session["error"] != null && (string)Session["error"] == "error")
    {
        <div id="target">
            @{
                Html.RenderPartial("_ErrorHome");
            }
        </div>
    }
    else
    {
        <h2>Resultados para < @Model.NombreSearch ></h2>
        <div id="target">
            @{
                Html.RenderPartial("Result_Details", Model);
            }
        </div>
     }

}


@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
<script src="~/Scripts/sidebar.js" type="text/javascript"></script>
<script src="~/scripts/jquery.unobtrusive-ajax.js" type="text/javascript"></script>
<script src="~/Scripts/home.js" type="text/javascript"></script>